{
  "name": "electron-updater-example3",
  "version": "0.7.9",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Glaadiss",
  "scripts": {
    "publish": "electron-builder build --win -p always",
    "electron": "electron ."
  },
  "devDependencies": {
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^4.3.4"
  },
  "build": {
    "appId": "com.github.glaadiss.electronupdaterexample3",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "dmg"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "glaadiss",
        "repo": "electron-updater-example"
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
